# Environment Configuration for KYC Service
NODE_ENV=development

# Testing Configuration
SKIP_AADHAAR_CHECKSUM=true

# Service Port
KYC_SERVICE_PORT=4001

# IDfy API Configuration (Production Ready)
IDFY_API_KEY=your_idfy_api_key_here
IDFY_ACCOUNT_ID=your_idfy_account_id_here
IDFY_BASE_URL=https://api.idfy.com
IDFY_WEBHOOK_SECRET=your_webhook_secret_here

# IDfy Authentication (Additional credentials)
IDFY_USERNAME=your_idfy_username_here
IDFY_PASSWORD=your_idfy_password_here

# Legacy KYC Configuration (for backward compatibility)
KYC_API_KEY=your_kyc_api_key_here
KYC_BASE_URL=https://api.idfy.com

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Database Configuration (Supabase)
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Encryption Keys
ENCRYPTION_KEY=your_32_character_encryption_key_here
JWT_SECRET=your_jwt_secret_key_here

# Internal API Key
INTERNAL_API_KEY=super_secure_admin_key_change_in_production

# Logging Configuration
LOG_LEVEL=info

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
