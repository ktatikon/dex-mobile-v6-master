# Environment Configuration
NODE_ENV=development

# Service Ports
KYC_SERVICE_PORT=4001
AML_SERVICE_PORT=4002

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Database Configuration (Supabase)
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Encryption Keys
ENCRYPTION_KEY=a1b2c3d4e5f6789012345678901234567890abcdef1234567890abcdef123456
JWT_SECRET=your_jwt_secret_key_here_change_in_production

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Logging Configuration
LOG_LEVEL=info

# KYC Provider Configuration - IDfy (Selected Provider)
KYC_API_KEY=e443e8cc-47ca-47e8-b0f3-da146040dd59
KYC_BASE_URL=https://apicentral.idfy.com
IDFY_API_KEY=e443e8cc-47ca-47e8-b0f3-da146040dd59
IDFY_BASE_URL=https://apicentral.idfy.com
IDFY_WEBHOOK_SECRET=your_idfy_webhook_secret

# IDfy Sandbox Configuration
IDFY_SANDBOX_MODE=true
IDFY_SANDBOX_API_KEY=sandbox_api_key_here
IDFY_SANDBOX_BASE_URL=https://sandbox.idfy.com

# AML Provider Configuration - IDfy Integration
AML_API_KEY=e443e8cc-47ca-47e8-b0f3-da146040dd59
AML_BASE_URL=https://apicentral.idfy.com
IDFY_AML_API_KEY=e443e8cc-47ca-47e8-b0f3-da146040dd59

# Internal API Keys
INTERNAL_API_KEY=super_secure_admin_key_change_in_production

# UIDAI Configuration (for Aadhaar)
UIDAI_AUA_CODE=your_aua_code
UIDAI_SUB_AUA_CODE=your_sub_aua_code
UIDAI_LICENSE_KEY=your_uidai_license_key

# NSDL Configuration (for PAN)
NSDL_API_KEY=your_nsdl_api_key
NSDL_BASE_URL=https://api.nsdl.com

# Local AML Lists
RBI_SANCTIONS_URL=https://rbi.org.in/sanctions-list
FIU_SANCTIONS_URL=https://fiuindia.gov.in/sanctions
UN_SANCTIONS_URL=https://scsanctions.un.org/api

# File Storage Configuration
SUPABASE_STORAGE_BUCKET=kyc-documents

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=30
STRICT_RATE_LIMIT_MAX_REQUESTS=5

# Queue Configuration
BULL_REDIS_HOST=localhost
BULL_REDIS_PORT=6379
BULL_REDIS_PASSWORD=

# Development/Testing
MOCK_PROVIDERS=false
SKIP_BIOMETRIC_VERIFICATION=false
ENABLE_TEST_ENDPOINTS=false
